{
	"background": {
    "persistent": false,
    "page": "background.html"
  },
  
  "manifest_version": 2,
  "icons": { "128": "images/icon-128.png", "16": "images/icon-16.png" },
  "name": "huoban",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
  	"cookies",
  	"notifications",
  	"alarms",
    "tabs",
    "webNavigation",
    "https://secure.flickr.com/",
    "*://*.huoban.com/"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}