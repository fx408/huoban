{
  "manifest_version": 2,
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": { "128": "images/icon-128.png", "16": "images/icon-16.png" },
  "name": "huoban.com",
  "description": "huoban.com",
  "version": "1.0.3",

  "permissions": [
  	"notifications",
  	"alarms",
    "tabs",
    "webNavigation",
    "cookies",
    "*://*.huoban.com/"
  ],
  "background": {
    "persistent": false,
    "page": "pages/background.html"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "pages/index.html"
  }
}